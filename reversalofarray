#include <stdio.h>
#include <stdlib.h>

int main()
{
    int temp;
    int num;
    scanf("%d",&num);
    int arr[num];

    /* Write the logic to reverse the array. */
        for (int k=0; k<num; k++) {
        scanf("%d",&arr[k]);
        }

        for (int i=0; i<=num/2; i++) {
        temp=arr[i];
        arr[i]=arr[num-i];
        arr[num-i]=temp;
         
        }
    for (int i=1;i<=num;i++) {
    printf("%d ",arr[i]);
    }
        
    return 0;
}
